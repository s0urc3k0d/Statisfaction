{
  "name": "@statisfaction/server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "make-admin": "node scripts/make_admin.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:switch-postgres": "copy prisma\\schema.postgresql.prisma prisma\\schema.prisma && prisma generate",
    "db:switch-sqlite": "git checkout prisma/schema.prisma && prisma generate",
    "migrate:data": "ts-node scripts/migrate-to-postgres.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.7",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^6.9.14",
    "pg": "^8.11.3",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.7.7",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
